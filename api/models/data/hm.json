  "notInStockSizes": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "ul.picker-list > li.item.out-of-stock > div > button > span:first-child"
  },
  "inStockSizes": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "ul.picker-list > li.item:not(.out-of-stock) > div > button > span:first-child"
  },
  "clickSize": "",
  "title": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "hm-product-name#js-product-name"
  },
  "category": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "//nav[@aria-label='Breadcrumb']/ol[count(li)=4]/li[last()-1]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=5]/li[last()-2]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=5]/li[last()-1]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=6]/li[last()-3]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=6]/li[last()-2]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=6]/li[last()-1]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=7]/li[last()-4]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=7]/li[last()-3]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=7]/li[last()-2]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=7]/li[last()-1]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=8]/li[last()-5]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=8]/li[last()-4]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=8]/li[last()-3]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=8]/li[last()-2]/a |  //nav[@aria-label='Breadcrumb']/ol[count(li)=8]/li[last()-1]/a"
  },
  "price": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "//hm-product-price[@id='product-price']/div/span[1]"
  },
  "color": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "h3.product-input-label"
  },
  "images": {
    "attribute": "src",
    "regex": "",
    "groups": [],
    "selector": "figure.pdp-image > div > img"
