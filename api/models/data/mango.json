{
  "isHeadless": true,
  "waitUntill": "domcontentloaded",
  "blockResourceTypes": ["image","media","font","other","fetch"],
  "blockUrls": [
    "google",
    "cookielaw",
    "go-mpulse",
    "fitanalytics",
    "onetrust",
    "akstat.io",
    "hotjar",
    "yandex",
    "tiktok",
    "st0.mngbcn.com/static/shop/desktop/custom.js",
    "shop.mango.com/st-header/header/desktop/styles",
    "shop.mango.com/st-newsletter/newsletter/footer/styles",
    "snapchat",
    "shop.mango.com/ws-personalization/api/pdp/v1/recommendations/",
    "shop.mango.com/ws-favorites/favorites/v1/favorites/check",
    "sc-static.net",
    "bing.com",
    "pixel.tapad.com",
    "shop.mango.com/services/newsletterConfiguration",
    "facebook.net",
    "doubleclick.net",
    "creativecdn.com",
    "st0.mngbcn.com/fukku",
    "filter=lastViewed",
    "shop.mango.com/ws-size-guide/sizeguide",
    "shop.mango.com/services/header-bag",
    "shop.mango.com/st-fukku",
    "st.mngbcn.com/scripts/optimize.js"
  ],
  "container": "div.product-detail-main",
  "notInStockSizes": {
    "attribute": "data-size",
    "regex": "",
    "groups": [],
    "selector": "div.selector-list > .size-unavailable"
  },
  "inStockSizes": {
    "attribute": "data-size",
    "regex": "",
    "groups": [],
    "selector": "div.selector-list > .size-available "
  },
  "clickSize": "",
  "title": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "h1.product-name"
  },
  "category": {
    "attribute": "",
    "regex": "[\\S ]+ ([\\S ]+)",
    "groups": [1],
    "selector": "h1.product-name"
  },
  "price": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "//div[@class='sticky-prices']/span/div[@data-testid='prices.wrapper']/span[last()]"
  },
  "color": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "span[itemprop='color']"
  },
  "images": {
    "attribute": "src",
    "regex": "",
    "groups": [],
    "selector": "div.product-images > ul > li > div > img"
  }

}