{
  "debug": true,
  "isHeadless": true,
  "waitUntill": "domcontentloaded",
  "blockResourceTypes": ["image","media","font","other"],
  "whiteListUrls": [
    "https://shop.mango.com",
    "mngbcn.com"
  ],
  "blockUrls": [
    "https://st0.mngbcn.com/static/shop/desktop/custom.css",
    "https://st0.mngbcn.com/static/styleguide",
    "https://st0.mngbcn.com/fukku/fukku.css",
    "https://shop.mango.com/javax.faces.resource",
    "https://st0.mngbcn.com/static/js/pluginsTOP.js"
  ],
  "container": "div.product-detail-main",
  "notInStockSizes": {
    "attribute": "data-size",
    "regex": "",
    "groups": [],
    "selector": "div#sizeSelector > div.selector-list > .size-unavailable"
  },
  "inStockSizes": {
    "attribute": "data-size",
    "regex": "",
    "groups": [],
    "selector": "div#sizeSelector > div.selector-list > .size-available"
  },
  "clickSize": "",
  "title": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "h1.product-name"
  },
  "category": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "//ol[@class='breadcrumbs']/li[last()-1]/a/span | //ol[@class='breadcrumbs']/li[last()]/a/span"
  },
  "price": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "//div[@class='sticky-prices']/span/div[@data-testid='prices.wrapper']/descendant::span[contains(text(),'TL')][last()]"
  },
  "color": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "span[itemprop='color']"
  },
  "images": {
    "attribute": "src",
    "regex": "",
    "groups": [],
    "selector": "div.product-images > ul > li > div > img"
  }

}