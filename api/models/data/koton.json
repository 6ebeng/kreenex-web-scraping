{
  "isHeadless": true,
  "waitUntill": "domcontentloaded",
  "blockResourceTypes": ["image","media","stylesheet","font","other","fetch","script"],
  "blockUrls": [
    "360yield.com",
    "sharethrough.com",
    "aralego.com",
    "outbrain.com",
    "socdm.com",
    "criteo.com",
    "krxd.net",
    "teads.tv",
    "bidswitch.net",
    "media.net",
    "bluekai.com",
    "casalemedia.com",
    "rlcdn.com",
    "smartadserver.com",
    "tiktok.com",
    "rubiconproject.com",
    "bing.com",
    "adnxs.com",
    "yahoo.com",
    "taboola.com",
    "3lift.com",
    "doubleclick.net",
    "usizy.com",
    "lilasignal.com",
    "visilabs.net",
    "segmentify.com",
    "hotjar.com",
    "twitter.com",
    "relateddigital.com",
    "google"
  ],
  "container": "div.product-info",
  "notInStockSizes": {
    "attribute": "data-value",
    "regex": "",
    "groups": [],
    "selector": "div.variant__options > div.variant__option.-disabled"
  },
  "inStockSizes": {
    "attribute": "data-value",
    "regex": "",
    "groups": [],
    "selector": "div.variant__options > div.variant__option:not(.-disabled)"
  },
  "clickSize": "",
  "title": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "h1.product-info__header-title"
  },
  "category": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "//div[contains(@class,'breadcrumb')]/a[last()]"
  },
  "price": {
    "attribute": "",
    "regex": "",
    "groups": [],
    "selector": "div.price > div.price__base > div.price__price"
  },
  "color": {
    "attribute": "",
    "regex": "(?<=Renk: ).+",
    "groups": [],
    "selector": "pz-variant[name='Renk'] > p > span"
  },
  "images": {
    "attribute": "data-src",
    "regex": "",
    "groups": [],
    "selector": "div.product-media__images > picture > img"
  }

}